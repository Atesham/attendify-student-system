{
    "version": 2,
    "routes": [
      {
        "src": "/[^.]+",
        "dest": "/",
        "status": 200
      },
      {
        "src": "/(.*)",
        "dest": "/404.html",
        "status": 404
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=0, must-revalidate"
          }
        ]
      }
    ]
  }